<div class="results-container">
    {{#if artists}}
    {{#each artists}}
    <div class="card">
        <div class="img-container">
            <img src="{{this.images.1.url}}" alt="{{this.name}}">
        </div>
        <div class="card-bottom">
            <h2>{{this.name}}</h2>
            <button>View Albums</button>
        </div>
    </div>
    {{/each}}
    {{else}}
    <div class=" no-artist">
        <p>No artist information found.</p>
    </div>
    {{/if}}
</div>