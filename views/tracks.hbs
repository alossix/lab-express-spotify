<div class="tracks-container">
    {{#if tracks}}
    {{#each tracks }}
    <h2>{{this.name}}</h2>
    <audio controls>
        <source src="{{this.preview_url}}" type="audio/mpeg">
    </audio>
    {{/each}}
    {{else}}
    <div class=" no-artist">
        <p>No artist information found.</p>
    </div>
    {{/if}}
</div>